    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "jimp": "^0.22.10",
    "nodemon": "^3.0.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "uuid": "^9.0.0",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
